man		#manual - info about the command
uname		#print system information
uname -r	#print the kernel release
lsb_release	#prints certain LSB (Linux Standard Base) and Distribution information
pwd		#print name of current/working directory
who		#show who is logged on
ls		#list directory contents
ls -l		#use a long listing format
ls -la		#use a long listing format + #do not ignore entries starting with .
cd		#change the working directory
clear/ctrl+L	#clear the terminal screen
cat		#concatenate files and print on the standard output
echo		#display a line of text
echo $0		#name of the running process - name of the shell or a script
echo -n		#do not output the trailing newline
echo -e		#enable interpretation of backslash escapes
echo -e \\     	#backslash
echo -e \a     	#alert (BEL)
echo -e	\b     	#backspace
echo -e	\c     	#produce no further output
echo -e	\e     	#escape
echo -e	\f     	#form feed
echo -e	\n     	#new line
echo -e	\r     	#carriage return
echo -e	\t     	#horizontal tab
echo -e	\v     	#vertical tab
echo $PATH	#display colon-delimited list of directories that the shell searchers for executable programs
whoami		#print effective userid
mkdir		#make directories
rmdir		#removes empty directories
rm -rf		#remove files or directories + #remove directories and their contents recursively + #ignore nonexistent files and arguments, never prompt
history 	#prints terminal history
nano		#summons nano editor
git-add 	#Add file contents to the index
git-clone	#Clone a repository into a new directory
git-commit	#Record changes to the repository
git-commit -m  	#Use the given <msg> as the commit message
chmod nnn	#set permissions r=4, w=2, x=1 ## chmod 744 7=owner, 4=group, 4=other
PATH=$PATH:/directory		#add a directory to the paths shell checks for executables, /directury is an example


